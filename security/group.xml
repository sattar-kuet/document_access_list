<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="module_category_my_users" model="ir.module.category">
            <field name="name">My User Roles</field>
            <field name="sequence">10</field>
        </record>

        <!-- Admin -->
        <record id="group_my_admin" model="res.groups">
            <field name="name">Admin</field>
            <field name="category_id" ref="module_category_my_users"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        </record>

        <!-- Staff -->
        <record id="group_my_staff" model="res.groups">
            <field name="name">Staff</field>
            <field name="category_id" ref="module_category_my_users"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        </record>

    </data>
</odoo>
